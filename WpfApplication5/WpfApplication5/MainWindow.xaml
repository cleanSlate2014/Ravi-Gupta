<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing" x:Class="WpfApplication5.MainWindow"
        Title="MainWindow" Height="350" Width="525">
	<Grid>
        
		<ed:BlockArrow Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="50" Margin="136,217,0,0" Orientation="Right" Stroke="Black" VerticalAlignment="Top" Width="100"/>
		<Glyphs FontRenderingEmSize="12" Fill="Black" HorizontalAlignment="Left" Height="100" Margin="48,99,0,0" UnicodeString="Glyphs" VerticalAlignment="Top" Width="100"/>
		<ed:LineArrow Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="1" Margin="136,163,0,0" Stroke="Black" VerticalAlignment="Top" Width="117"/>
		<ed:RegularPolygon Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="18.87" InnerRadius="1" Margin="136.246,188.78,0,0" PointCount="3" Stretch="Fill" Stroke="Black" VerticalAlignment="Top" Width="18.124" RenderTransformOrigin="0.5,0.5">
			<ed:RegularPolygon.RenderTransform>
				<TransformGroup>
					<ScaleTransform/>
					<SkewTransform/>
					<RotateTransform Angle="91.209"/>
					<TranslateTransform/>
				</TransformGroup>
			</ed:RegularPolygon.RenderTransform>
		</ed:RegularPolygon>
        
	</Grid>
</Window>
